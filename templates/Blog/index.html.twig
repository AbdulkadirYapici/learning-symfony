

{%  if blog is defined %}
  {% for blogdata in blog %}
    {{ blogdata.id }}-{{ blogdata.entry }}- {{ blogdata.gorsel }} = <a href="{{ asset('uploads/afisler/' ~ blogdata.gorsel) }}"> Afiş </a>
    <form action="{{ url('deletecsrf_entry', {id: blogdata.id}) }}" method="post">
      <input name = "token" type="hidden" value="{{ csrf_token('entry-sil') }}">
      <button type="submit">Sil</button>
    </form>
    <br>
  {% endfor %}
  {{ dump(blog) }}
{% endif %}
{% if is_granted('ROLE_ADMIN') %}
<b>Bu yazıyı sadece admin görür</b>
{% endif %}

<a href="{{ path('form') }}">Link to Form Page</a>